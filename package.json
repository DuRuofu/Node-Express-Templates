{
  "name": "node-koa-templates",
  "version": "1.0.0",
  "description": "Using server templates for koa and prisma.",
  "main": "src/app.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --config .prettierrc . --write",
    "prepare": "husky install",
    "cz": "cz",
    "updatadb": "npx prisma migrate dev --schema ./src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "DuRuofu",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.0",
    "koa-body": "^6.0.1",
    "koa-parameter": "^3.0.1",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-swagger-ui": "^5.10.0",
    "log4js": "^6.9.1",
    "pm2": "^5.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/ip": "^1.1.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.14.0",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/koa2-cors": "^2.0.5",
    "@types/log4js": "^2.3.5",
    "@types/node": "^20.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.1",
    "prisma": "^5.8.0"
  }
}
